{
  "manifest_version": 2,
  "name": "CHROME TALK",
  "description": "Simple video chat application powered by WebRTC.",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "storage",
    "videoCapture",
    "audioCapture"
  ]
}
